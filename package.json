{
  "name": "auth0-express-webapp-sample",
  "scripts": {
    "build": "npm run clean && npx tsc && npm run copy-views",
    "clean": "shx rm -rf dist",
    "copy-views": "shx cp -r src/views src/public dist",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^6.1.0",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-openid-connect": "^2.17.0",
    "http-errors": "~1.8.1",
    "pug": "^3.0.3",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/pug": "^2.0.10",
    "@types/qrcode": "^1.5.5",
    "shx": "^0.3.4",
    "typescript": "^5.6.3"
  }
}
